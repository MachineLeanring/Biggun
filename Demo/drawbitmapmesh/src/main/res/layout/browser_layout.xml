<?xml version="1.0" encoding="utf-8"?>
<com.biggun.drawbitmapmesh.View.RecyclerViewLayout xmlns:android="http://schemas.android.com/apk/res/android"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">
    <ImageView
        android:id="@+id/browser_img"
        android:layout_alignParentLeft="true"
        android:layout_centerVertical="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"/>
    <TextView
        android:id="@+id/browser_tv"
        android:layout_toRightOf="@id/browser_img"
        android:layout_centerVertical="true"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
</com.biggun.drawbitmapmesh.View.RecyclerViewLayout>